
public class Estrelas {

	public static void main(String[] args) {

		for ( int i = 0 ; i < 10 ; i = i + 1 ) {
			System.out.println("**********");
		}
		
		int x = 1;
		
		while (x <= 10) {
			System.out.println("++++++++++");
			x = x + 1;
		}
		
		int y = 1;
		do { 
			System.out.println(">>>>>>>>>>>>>");
			y = y + 1;
		} while (y <= 10);
		
		
	
	}
	
}
