package helloworld;

import java.util.Scanner;

public class SomeProg {

	public static void main(String[] args) { 	
		
		Scanner keyboard = new Scanner(System.in);
		
		System.out.print("Qual a sua idade? ");
		int idade = keyboard.nextInt();

		System.out.print("Tem título (S/N)? ");
		String temTitulo = keyboard.nextLine();

		if ((idade > 15) && (temTitulo == "S")) {
			System.out.println("Você já pode votar!");
		}
		else {
			System.out.println("Tire o dia de folga!");
		}
		
		System.out.println("Boa noite!");
		
	}
	
}